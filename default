ssl_certificate /etc/ssl/certs/sgp1.jcharante.com.pem;
ssl_certificate_key /etc/ssl/private/sgp1.jcharante.com.key;

server {
	listen 80;
	server_name *.sgp1.jcharante.com;
	return 301 https://$host$request_uri;
}

server {
	listen 443 ssl;
	server_name vta-b.sgp1.jcharante.com;
	ssl on;
	
	location / {
		proxy_pass http://172.18.0.1:3100;
	}
}